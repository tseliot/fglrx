#DRI stuff
arch/x86/usr/X11R6/lib/libAMD*    usr/lib/fglrx

#debian integration
debian/overrides/fglrx-updates                   usr/share/lintian/overrides
debian/acpi/fglrx-*-aticonfig                   etc/acpi/events
debian/acpi/fglrx-powermode.sh                  etc/acpi
debian/10fglrx                                  usr/lib/fglrx
etc/ati/*                                       usr/lib/fglrx/etc/ati
debian/replacements/authatieventsd.sh           usr/lib/fglrx/etc/ati
arch/x86/etc/OpenCL/vendors/*.icd         usr/lib/fglrx/etc/OpenCL/vendors
arch/x86_64/etc/OpenCL/vendors/*.icd    usr/lib/fglrx/etc/OpenCL/vendors

#Blacklist some card ids from GRUB gfxpayload=keep
